    "happy-dom": "^11.0.3",
    "miragejs": "^0.1.47",
    "vite": "^2.5.4",
    "vitest": "^0.34.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "es2022": true
