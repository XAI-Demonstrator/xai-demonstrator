{
  "name": "inspection-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "node download_assets.js && vue-cli-service serve",
    "build": "node download_assets.js && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --collect-coverage",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@xai-demonstrator/xaidemo-ui": "^0.3.2",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "core-js": "^3.9.1",
    "debounce": "^1.2.1",
    "easy-bem": "^1.1.1",
    "flat": "^5.0.2",
    "vue": "^2.6.11",
    "vue-advanced-cropper": "1.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-service": "^4.5.12",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.22.0",
    "eslint-plugin-vue": "^7.7.0",
    "flush-promises": "^1.0.2",
    "miragejs": "^0.1.41",
    "request": "^2.88.2",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
