{
  "name": "inspection-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "node download_assets.js && vue-cli-service serve",
    "build": "node download_assets.js && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --collect-coverage",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@xai-demonstrator/xaidemo-ui": "^0.4.6",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "core-js": "^3.22.7",
    "debounce": "^1.2.1",
    "easy-bem": "^1.1.1",
    "flat": "^5.0.2",
    "vue": "^3.2.36",
    "vue-advanced-cropper": "^2.8.3",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^4.2.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.2.38",
    "@vue/test-utils": "^2.0.2",
    "@vue/vue3-jest": "^28.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.20.0",
    "flush-promises": "^1.0.2",
    "jest": "^27.1.0",
    "miragejs": "^0.1.45",
    "request": "^2.88.2",
    "sass": "^1.52.1",
    "sass-loader": "^10.3.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
