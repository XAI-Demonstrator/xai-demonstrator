{
  "name": "@xai-demonstrator/xaidemo-ui",
  "version": "0.2.3",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:app": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --collect-coverage",
    "lint": "vue-cli-service lint",
    "build:lib": "vue-cli-service build --target lib --dest lib src/index.js"
  },
  "main": "./lib/xaidemo-ui.common.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "Apache-2.0",
  "repository": "https://github.com/xai-demonstrator/xai-demonstrator.git"
}
